{
	"name": "muxmuxmux",
	"private": true,
	"version": "0.1.0",
	"description": "Typesafe state-dependent behavior for modern apps",
	"author": "Vadim Kotov",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/vadirn/muxmuxmux"
	},
	"homepage": "https://github.com/vadirn/muxmuxmux#readme",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"build": "pnpm -r run build",
		"dev": "pnpm -r --parallel run dev",
		"typecheck": "pnpm -r run typecheck",
		"test": "pnpm -r run test",
		"size": "pnpm -r run size",
		"clean": "pnpm -r run clean && rm -rf node_modules",
		"format": "biome check --write .",
		"lint": "biome lint ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"@changesets/cli": "2.29.7",
		"typescript": "5.9.3"
	},
	"packageManager": "pnpm@10.21.0"
}
